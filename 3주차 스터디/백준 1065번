#include<stdio.h>

int main(void)
{
	int num;
	int arr[4];
	int count=0;
	scanf("%d", &num);
	for (int i = 1;i <= num;i++)
	{
		if (i < 100)
			count++;
		else if(i<1000)
		{
			arr[0] = i / 100;
			arr[1] = (i %100)/10;
			arr[2] = i % 10;
			if (arr[0]-arr[1]==arr[1]-arr[2])
				count++;
		}		

	}
	printf("%d", count);
}
